<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.loginmapper.LoginMapper">
	<select id="getLogin" resultType="com.spring.loginvo.LoginVo" parameterType="String">
		SELECT 
			NAME 
		FROM 
			LOGIN 
		WHERE login_id = #{id}
	</select>	
</mapper>